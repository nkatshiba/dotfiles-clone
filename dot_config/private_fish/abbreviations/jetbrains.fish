# jetbrains [rider / pycharm / clion]
abbr -a rider 'export _JAVA_AWT_WM_NONREPARENTING=1 && hyprctl dispatch exec rider && kitten @ close-window --self'
abbr -a pycharm 'export _JAVA_AWT_WM_NONREPARENTING=1 && hyprctl dispatch exec pycharm-professional && kitten @ close-window --self'
abbr -a clion 'export _JAVA_AWT_WM_NONREPARENTING=1 && hyprctl dispatch exec clion && kitten @ close-window --self'
