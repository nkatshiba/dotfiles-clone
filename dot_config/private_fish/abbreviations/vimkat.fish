# Vimkat (VK)
abbr -a vk '$HOME/src/vimkat'
abbr -a vko 'vimkat-open'

abbr -a vknc 'j $HOME/.local/share/nvim/lazy/citruszest.nvim/lua/citruszest/palettes/ && cme colors.lua'

# services
abbr -a bit 'hyprctl dispatch exec $HOME/.local/nativefier/vimkat-bit-linux-x64/vimkat-bit'
abbr -a vmovs 'vimkat-movies'
abbr -a vjelly 'hyprctl dispatch exec qutebrowser -- --target window "https://jelly.vimkat.com/"'
abbr -a jelly 'hyprctl dispatch exec $HOME/.local/nativefier/vimkat-jellyfin-linux-x64/vimkat-jellyfin && exit'
